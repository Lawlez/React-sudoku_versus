{   
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-transform-regenerator",
    "syntax-async-functions"
  ]
}